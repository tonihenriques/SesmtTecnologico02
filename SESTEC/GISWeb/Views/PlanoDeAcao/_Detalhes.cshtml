@model GISModel.Entidades.PlanoDeAcao

<table id="dynamic-table" class="table table-striped table-bordered table-hover">
        

    <tbody>
        <tr>
            <td><strong>Data da Criação</strong></td>
            <td>@Model.DataInclusao.ToString("dd/MM/yyyy")</td>
        </tr>
        <tr>
            <td><strong>Departamento</strong></td>
            <td>@Model.Gerencia</td>
        </tr>
        <tr>
            <td><strong>Tipo do Plano de Ação</strong></td>
            <td>@Model.TipoDoPlanoDeAcao</td>
        </tr>
        <tr>
            <td><strong>Descrição</strong></td>
            <td>@Model.DescricaoDoPlanoDeAcao</td>
        </tr>
        <tr>
            <td><strong>Responsável</strong></td>
            <td>@Model.Responsavel</td>
        </tr>
        <tr>
            <td><strong>Previsão</strong></td>
            <td>@Model.DataPrevista.ToString("dd/MM/yyyy")</td>
        </tr>

        @if (ViewBag.DataExclusao != ViewBag.data)
    {
        <tr>
            <td><strong>Concluído em:</strong></td>
            <td>@Model.DataExclusao.ToString("dd/MM/yyyy")</td>
        </tr>

}
else
{

    <tr>
        <td><strong>Concluído em:</strong></td>
        <td></td>
    </tr>

}


        <tr>
            <td><strong>Entregue</strong></td>
            <td>@Model.Entregue</td>
        </tr>
        <tr>
            <td><strong>Responável pela entrega</strong></td>
            <td>@Model.ResponsavelPelaEntrega</td>
        </tr>

</table>


