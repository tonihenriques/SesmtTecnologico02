@model GISModel.Entidades.Admissao

@if (Model != null)
{

<table class="table table-striped table-bordered table-hover">
    <tr>
        <td><img src="/Images/AdmissaoImagens/@Model.IDAdmissao/Admissao.Imagem" alt="Admissao" width="200px" /> </td>
    </tr>
    <tr>
        <td><strong>Nome</strong></td>
        <td>@Model.Empregado.Nome</td>
    </tr>
    <tr>
        <td><strong>CPF</strong></td>
        <td>@Model.Empregado.CPF</td>
    </tr>
    <tr>
        <td><strong>Email</strong></td>
        <td>@Model.Empregado.Email</td>
    </tr>
    <tr>
        <td><strong>Data de Nascimento</strong></td>
        <td>@Model.Empregado.DataNascimento</td>
    </tr>
    <tr>
        <td><strong>Empresa Atual</strong></td>
        <td>@Model.Empresa.NomeFantasia</td>
    </tr>


</table>
}


   

      

    @section Scripts {
        @Scripts.Render("~/bundles/jQ_dataTable")

<script src="~/Scripts/Custom/Admissao/Listagem.js"></script>
        
    }




































